package com.giacomini.andrea.EserciziSupplementari.Modulo1;

public class Esercizio1 {

	public static void main(String[] args){
		
		System.out.println("Hello World! Giono di Pasquetta. 123");
		
		// Stampa una String;
		System.out.println("123");
		
		// Stampa la somma tra due numeri;
		System.out.println(123 + 321);
	}
}