abstract class TestAbstractClass {
	
	// Metodo d'istanza;
	public void instanceMethodAbstractClass(){
		
	}
	
	// Metodo STATIC;
	public static void staticMethodAbstractClass(){
		
	}
	
	// Metodo abstract;
	public abstract void abstractMethodAbstractClass();
}
